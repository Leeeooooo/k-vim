{
  "diagnostic.displayByAle": false,
  "diagnostic.enableMessage": "always",
  "colors.enable": true,
  //"suggest.maxCompleteItemCount":8,
  // Enable preselect feature on neovim, default: `true`
  //"suggest.enablePreselect": true,
  // Add preview option to `completeopt`, default: `false`
  //"suggest.enablePreview": true,
  // completion automatically select the first completed
  "suggest.noselect": true,
  // "suggest.triggerAfterInsertEnter": true
  "suggest.minTriggerInputLength": 2,
  "python.formatting.provider": "black",
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": ["--ignore=E121,E123,E126,E226,E24,E704,W503", "--max-line-length=120"],
  "python.linting.pylintEnabled": true,
  "python.linting.pylintArgs": ["--ignore=E121,E123,E126,E226,E24,E704,W503", "--max-line-length=120"],
  // Target to show hover information, default is floating window when possible.
  //"coc.preferences.hoverTarget": "preview",
  // Auto close preview window on cursor move.,  default: `true`
  //"coc.preferences.previewAutoClose": false,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": "‚ùØ ",
  "diagnostic.floatConfig": {
    "border": true,
    "rounded": true,
    "maxWidth": 80
  },
  "hover.floatConfig": {
    "border": true,
    "rounded": true,
    "maxWidth": 80
  },
  "signature.floatConfig": {
    "border": true,
    "rounded": true
  },
  "languageserver": {},
